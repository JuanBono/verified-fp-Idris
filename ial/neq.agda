module ial.neq where

open import ial.eq
open import ial.negation

----------------------------------------------------------------------
-- syntax
----------------------------------------------------------------------

infix 4 _≢_ 

----------------------------------------------------------------------
-- defined types
----------------------------------------------------------------------

_≢_ : ∀ {ℓ}{A : Set ℓ} → A → A → Set ℓ
x ≢ y = ¬ (x ≡ y)